/**
 * Implements hook_help().
 */
function mediateca_help($route_name, \Drupal\Core\Routing\RouteMatchInterface $route_match){
    $output = '';
    switch ($route_name){
        case 'help.page.mediateca':
            $output .= '<p>Esta es la pagina de ayuda del modulo Escuela</p>';
    }
    return $output;
}